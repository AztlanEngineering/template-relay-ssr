{
  "verbose": false,
  "ignore": ["node_modules", "src/translations"],
  "env": {
    "NODE_ENV": "development",
    "BABEL_DISABLE_CACHE": 1,
    "LOCAL":true,
    "SSR":true
  },
  "execMap": {
    "js": "babel-node -r dotenv/config --stack-trace-limit=1000 --config-file ./babel.config.js"
  },
  "ext": ".js,.md,.graphql, .jsx",
  "watch": "./src/"
}

