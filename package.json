{
  "name": "@pareto-engineering/template-react",
  "version": "0.0.1-alpha.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:stylelint && npm run test:eslint",
    "start:dev": "webpack serve --config ./webpack.client.dev.js",
    "start:prod": "webpack serve --config ./webpack.client.prod.js",
    "start": "npm run start:dev",
    "compile:client:dev": "webpack --config ./webpack.client.dev.js",
    "compile:client:prod": "NODE_ENV=production webpack --config ./webpack.client.prod.js",
    "ccd": "npm run compile:client:dev",
    "ccp": "npm run compile:client:prod",
    "relay": "relay-compiler",
    "check-types": "tsc",
    "install:hooks": "git config --local core.hooksPath .githooks/",
    "fetch-schema:dev": "node scripts/fetchSchema.js",
    "fetch-schema:prod": "node scripts/fetchSchema.js -- --ref production",
    "fetch-schema": "npm run fetch-schema:prod",
    "reset:generated": "rm -rf ./**/__generated__",
    "testconfig:eslint": "eslint src/**/*.jsx",
    "testconfig:stylelint": "stylelint src/styles/local.scss",
    "test:eslint": "eslint --ext .jsx,.js src/",
    "test:stylelint": "stylelint src/**/*.scss --rd --rdd --risd --color --formatter verbose",
    "test":"npm run test:stylelint && npm run test:eslint",
    "fix:stylelint": "npm run test:stylelint -- --fix",
    "fix:eslint": "npm run test:eslint -- --fix",
    "link:ds": "npm link @pareto-engineering/design-system",
    "ssr": "NODE_ENV=development SSR=true webpack --watch --config ./webpack.ssr.server.js",
    "compile:renderer": "NODE_ENV=production SSR=true webpack --config ./webpack.ssr.renderer.js",
    "build-all": "npm run compile:client:prod && npm run compile:renderer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HelloPareto/template-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HelloPareto/template-react/issues"
  },
  "homepage": "https://github.com/HelloPareto/template-react#readme",
