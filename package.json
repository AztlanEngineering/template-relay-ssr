    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-relay": "11.0.1",
    "react-relay-network-modern": "^6.0.0",
    "react-router-dom": "^5.2.0",
    "react-ssr-prepass": "^1.4.0",
    "relay-runtime": "^11.0.1"
  },
